{
  "name": "mafia-insight",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "type-check": "tsc --noEmit",
    "format": "prettier --write .",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:unit": "vitest run tests/unit/",
    "test:integration": "vitest run tests/integration/",
    "test:component": "vitest run tests/components/",
    "test:performance": "vitest run tests/performance/",
    "test:all": "npm run test:unit && npm run test:integration && npm run test:component && npm run test:e2e",
    "test:ci": "vitest run --reporter=verbose --reporter=json --reporter=junit --outputFile=test-results.json",
    "test:coverage": "vitest run --coverage",
    "test:coverage:ui": "vitest run --coverage --ui",
    "test:coverage:report": "vitest run --coverage --reporter=verbose --reporter=json --reporter=html",
    "test:coverage:dashboard": "node scripts/coverage-dashboard.js",
    "test:ui": "vitest --ui",
    "test:e2e": "playwright test",
    "test:e2e:quick": "playwright test --config=playwright.quick.config.ts",
    "test:e2e:ui": "playwright test --ui",
    "test:e2e:ci": "playwright test --reporter=json --outputFile=test-results-e2e.json",
    "test:validate": "npm run test:ci && npm run test:e2e:ci",
    "db:generate": "prisma generate",
    "db:migrate": "prisma migrate dev",
    "db:deploy": "prisma migrate deploy",
    "db:seed": "prisma db seed",
    "db:reset": "prisma migrate reset",
    "db:studio": "prisma studio",
    "prepare": "husky"
  },
