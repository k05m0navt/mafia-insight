    "openapi-types": "^7.0.0",
    "postcss": "^8.4.0",
    "prettier": "^3.0.0",
    "prisma": "^5.0.0",
    "shadcn": "^3.5.0",
    "tailwindcss": "^3.3.0",
    "tailwindcss-animate": "^1.0.7",
